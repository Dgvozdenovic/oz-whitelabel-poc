    "default": "npm run clean && npm run template:default && npm run dev",
    "dox": "npm run clean && npm run template:dox && npm run dev",
    "bp": "npm run clean && npm run template:bp && npm run dev",
    "all": "npm run clean && npm run template:all && npm run dev"
